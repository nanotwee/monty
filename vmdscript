set num 50
for {set i 5} {$i < $num} {incr i} {
		# go to the given frame
		animate goto $i
		# take the picture
		set filename [format "%02d" $i].dat
	render Tachyon $filename
	}



convert -delay 30 -loop 0 ?eq.ppm 1eq.ppm 1eq.ppm 1eq.png 1eq.png 1eq.png 1eq.png 1ss.ppm 1ss.ppm ?ss.ppm 10ss.ppm 11ss.ppm 12ss.ppm 13ss.ppm 15ss.png 15ss.png ??ss.png 37ss.ppm 38ss.ppm 39ss.ppm 40ss.ppm EOflow2.gif


